{
	"name": "periodicjs",
	"description": "Customizable CMS platform",
	"version": "1.2.0",
	"main": "index.js",
	"engines": {
		"node": ">=0.10.x"
	},
	"author": {
		"name": "Yaw Joseph Etse",
		"email": "yaw.etse@gmail.com",
		"url": "http://about.me/yawjosephetse"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/typesettin/periodicjs.git"
	},
	"scripts": {
		"nd": "nodemon --watch app --watch content/extensions/restart.json index.js ",
		"startdev": "pm2 start content/config/process/development-dev.json",
		"start_devtest": "pm2 start content/config/process/development-test.json",
		"start_devclient": "pm2 start content/config/process/development-client.json",
		"start_prod": "pm2 start content/config/process/production.json",
		"start_prodclient": "pm2 start content/config/process/production-client.json",
		"start": "pm2 start content/config/process/development.json",
		"stop": "pm2 delete periodicjs",
		"reload": "pm2 reload periodicjs",
		"list": "pm2 list periodicjs",
		"test": "mocha -R spec --recursive",
		"postinstall": "node scripts/install.js"
	},
	"dependencies": {
		"async": "0.9.0",
		"bcrypt": "~0.7.8",
