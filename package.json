		"formidable": "1.0.15",
		"fs-extra": "0.9.1",
		"method-override": "2.0.2",
		"moment": "^2.7.0",
		"mongoose": "3.8.12",
		"morgan": "1.1.1",
		"multer": "0.1.0",
		"npm": "1.4.15",
		"optimist": "0.6.1",
		"passport": "~0.2.0",
		"periodicjs.core.controllerhelper": "1.1.1",
		"periodicjs.core.extensions": "1.5.1",
		"periodicjs.core.mailer": "1.2.0",
		"periodicjs.core.utilities": "1.1.2",
		"prompt": "0.2.13",
		"response-time": "2.0.0",
